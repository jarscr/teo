{
    "name": "jarscr/teo",
    "type": "project",
    "description": "TEO Simple PHP Framework for building web applications in PHP",
    "keywords": ["mvc","framework","php","costa rica","web","jarscr","teo"],
    "homepage": "https://github.com/jarscr/teo",
    "license": "MIT",
    "version":"1.1.1",
    "authors": [
        {
            "name": "Alfredo Rodr√≠guez",
            "email": "jarscr@gmail.com",
            "homepage": "http://www.jarscr.com",
            "role": "Developer"
        }
    ],
    "require": {
        "delight-im/auth": "^8.3",
        "mpdf/mpdf": "^8.2",
        "twig/twig": "^3.8",
        "symfony/translation": "^7.0",
        "symfony/twig-bridge": "^7.0"
    },
    "require-dev": {
        "sentry/sdk": "^3.6.0",
        "phpunit/phpunit": "^9.6.15"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
             "Core\\": "Core/",
             "App\\": "App/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
