    "require": {
        "php": "~7.3",
        "twig/twig": "~3.0",
        "symfony/polyfill-mbstring": "^1.10",       
        "psr/log": "^1.1",
        "mpdf/mpdf": "^8.0",
        "delight-im/auth": "^8.2",
        "symfony/yaml": "^5.1"
    },
    "require-dev": {
        "sentry/sdk": "^3.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
